<!DOCTYPE html>
<html lang="tr" class="no-js">

<head>
  <!-- Mobile Specific Meta -->
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <!-- Favicon-->
  <link rel="shortcut icon" href="~/ui/img/fav.png">
  <!-- Author Meta -->
  <meta name="author" content="CodePixar">
  <!-- Meta Description -->
  <meta name="description" content="">
  <!-- Meta Keyword -->
  <meta name="keywords" content="">
  <!-- meta character set -->
  <meta charset="UTF-8">
  <!-- Site Title -->
  <title>Karma Shop</title>
  <!--
  CSS
  ============================================= -->
  @await Html.PartialAsync("_StylesPartial")
  @await RenderSectionAsync("StylesSection", false)
</head>

<body>
  @await Component.InvokeAsync("NToastNotify")
  @await Html.PartialAsync("_HeaderPartial")

  @RenderBody()

  @await Html.PartialAsync("_FooterPartial")
  @await Html.PartialAsync("_ScriptsPartial")
  @await RenderSectionAsync("ScriptsSection", false)
</body>

</html>